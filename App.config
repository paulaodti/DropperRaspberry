<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--Mqtt Protocol config settings-->
    <add key="MqttHost" value="192.168.43.15" />
    <!--<add key="MqttHost" value="192.168.1.101" />IP quando está conectado ao Wifi de casa-->
    <!--broker configurations-->
    <add key="MqttClientName" value="raspberry" />
    <add key="MqttPort" value="1883" />
    <add key="MqttUsername" value="dropper" />
    <add key="MqttPassword" value="henriquepaulo" />
    <!--acrescente mais tópicos separando com ';'-->
    <add key="MqttTopics" value="Dropper/Sensor1;Dropper/Sensor2" />
  </appSettings>
</configuration>